set expandtab
set tabstop=4
set softtabstop=4
set shiftwidth=4
set laststatus=2
filetype plugin indent on
syntax on
let g:loaded_matchparen= 1
let mapleader=" "

nmap <Tab> :Lexplore<CR>
let g:netrw_keepdir = 0
let g:netrw_banner = 0
let g:netrw_winsize = 20
let g:netrw_localcopydircmd = 'cp -r'
let g:netrw_liststyle = 3
